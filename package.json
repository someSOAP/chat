{
  "name": "chat",
  "version": "1.0.0",
  "description": "simple js chat ",
  "main": "dist/index.js",
  "author": "someSOAP",
  "license": "ISC",
  "scripts": {
    "start":      "ts-node server/index.ts",
    "dev":        "npm run dev:server && npm run dev:front",
    "dev:server": "nodemon --exec \"npm start\" --watch server",
    "dev:front":  "nodemon --exec \"npm run build:dev\" --watch front",
    "build:dev":  "webpack --config webpack.config.js --mode=development",
    "buidl":      "webpack --config webpack.config.js --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/someSOAP/chat.git"
  },
  "dependencies": {
    "@types/node": "^14.0.5",
    "antd": "^4.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.2.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "babel-eslint": "^10.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "ip": "^1.1.5",
    "nodemon": "^2.0.4",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ext": "ts,tsx,html",
    "ignore": [
      "test/*",
      "docs/*",
      "dist/*",
      "node_modules/*"
    ]
  },
  "keywords": [
    "chat",
    "nodejs",
    "react",
    "js",
    "crud"
  ],
  "bugs": {
    "url": "https://github.com/someSOAP/chat/issues"
  },
  "homepage": "https://github.com/someSOAP/chat#readme"
}
